<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">
    <TextView
        android:id="@+id/fragment2_item_textview1"
        style="@style/textview_indent_style"
        android:text="订单号" />

    <TextView
        android:id="@+id/fragment2_item_textview2"
        style="@style/textview_indent_style"
        android:text="批次号" />
    <TextView
        android:id="@+id/fragment2_item_textview3"
        style="@style/textview_indent_style"
        android:text="生产线" />

    <TextView
        android:id="@+id/fragment2_item_textview4"
        style="@style/textview_indent_style"
        android:text="出口评审号" />

    <TextView
        android:id="@+id/fragment2_item_textview5"
        style="@style/textview_indent_style"
        android:text="出口国家" />

    <TextView
        android:id="@+id/fragment2_item_textview6"
        style="@style/textview_indent_style"
        android:text="成品代码" />
    <TextView
        android:id="@+id/fragment2_item_textview7"
        style="@style/textview_indent_style"
        android:text="定制化订单"
        />

    <TextView
        android:id="@+id/fragment2_item_textview8"
        style="@style/textview_indent_style"
        android:text="订单数量" />
    <TextView
        android:id="@+id/fragment2_item_textview9"
        style="@style/textview_indent_style"
        android:text="完成数量" />
    <TextView
        android:id="@+id/fragment2_item_textview10"
        style="@style/textview_indent_style"
        android:text="完成率" />
    <TextView
        android:id="@+id/fragment2_item_textview11"
        style="@style/textview_indent_style"
        android:text="完成日期" />
    <TextView
        android:id="@+id/fragment2_item_textview12"
        style="@style/textview_indent_style"
        android:text="备注"
        />
</LinearLayout>